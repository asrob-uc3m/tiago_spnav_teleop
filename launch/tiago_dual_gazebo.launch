<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="end_effector_left" default="pal-gripper"/>
    <arg name="end_effector_right" default="pal-gripper"/>
    <arg name="gui" default="true"/>
    <arg name="debug" default="false"/>
    <arg name="extra_gazebo_args" default=""/>
    <arg name="base_type" default="pmb2"/>

    <include file="$(find tiago_dual_gazebo)/launch/tiago_dual_gazebo.launch">
        <arg name="end_effector_left" default="$(arg end_effector_left)"/>
        <arg name="end_effector_right" default="$(arg end_effector_right)"/>
        <arg name="gui" default="$(arg gui)"/>
        <arg name="debug" default="$(arg debug)"/>
        <arg name="public_sim" value="true"/>
        <arg name="tuck_arm" default="false"/>
        <arg name="base_type" default="$(arg base_type)"/>
        <arg name="extra_gazebo_args" value="$(arg extra_gazebo_args)"/>
    </include>

    <include file="$(find spacenav_node)/launch/classic.launch">
    </include>

    <include file="$(find tiago_spnav_teleop)/launch/spnav_controller_dual_right.launch">
    </include>
</launch>
